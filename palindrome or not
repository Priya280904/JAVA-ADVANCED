import java.util.*;
public class Main {
    public static void main(String[] args) {
        Scanner sc = new Scanner(System.in);
        int i = sc.nextInt();
        int sum = 0;
        int temp =i;
        while (i > 0) {
            int r = i % 10;
            sum = sum * 10 + r;
            i = i / 10;
        }
        if (sum==temp){
          System.out.println("palindrome");
        }
        else{
          System.out.println("not a palindrome");
        }
    }
}
