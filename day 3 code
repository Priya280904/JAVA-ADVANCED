6.code:
 import java.util.*;
public class Main
{
public static void main(String[] args) {
   Scanner sc=new Scanner(System.in);
    int size = sc.nextInt();
    int arr[] = new int[size];
   
    for (int i=0;i<size;i++){
      arr[i]=sc.nextInt();
    }
     int sum = 0;
  for (int i=0;i<size;i++){
    System.out.println(arr[i]+" ");
    sum = sum + arr[i];
  }

        System.out.println(sum);
}
}
7code:
import java.util.*;

public class Main {
    public static void main(String[] args) {

        Scanner sc = new Scanner(System.in);
        int size = sc.nextInt();
        int arr[] = new int[size];

        int evenMul = 1;
        int oddMul = 1;

        for (int i = 0; i < size; i++) {
            arr[i] = sc.nextInt();

            if (arr[i] % 2 == 0) {
                evenMul = evenMul * arr[i];
            } else {
                oddMul = oddMul * arr[i];
            }
        }

        System.out.println("Even Multiplication = " + evenMul);
        System.out.println("Odd Multiplication = " + oddMul);
    }
}


code 8
import java.util.*;
public class Main
{
public static void main(String[] args) {
   Scanner sc=new Scanner(System.in);
    int size = sc.nextInt();
    int arr[] = new int[size];
   
    for (int i=0;i<size;i++){
      arr[i]=sc.nextInt();
    }
    
        int max = arr[0];   
        for (int i = 0; i < size; i++) {
            if (arr[i] > max) {
                max = arr[i];
            }
        }

        System.out.println("max=" + max);
}
}
