1.code:
    import java.util.*;
public class Main
{
public static void main(String[] args) {
   Scanner sc=new Scanner(System.in);
 int a=sc.nextInt();
 int sum=0,temp=a;
 while(a>0){
     int r=a%10;
     sum=sum+(r*r*r);
        a=a/10;
 }
 if(sum==temp){
   System.out.println("armstrong number");
 }
else{
  System.out.println("not armstrong number");
}
}
}
